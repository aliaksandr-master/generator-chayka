            <div class="form_field <%= fieldClass %> <%= _.underscored(fieldName) %>" data-form-field="<%= fieldName %>">
                <label class="<%= labelClass %>"><%= fieldLabel %></label><select data-ng-init="options.<%= fieldName %>='<%= fieldDefault %>'; <%= fieldName %>Options = {'1': 'Yes', '0': 'No'}" data-ng-model="options.<%= fieldName %>" data-ng-options="key as value for (key, value) in <%= fieldName %>Options" title="<%= fieldLabel %>"></select>
            </div>
