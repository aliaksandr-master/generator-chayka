            <div class="form_field <%= fieldClass %>" data-form-field="<%= fieldName %>">
                <label class="<%= labelClass %>"><%= fieldLabel %></label><select data-ng-init="options.<%= fieldName %>='reload'; <%= fieldName %>Options = {'value1': 'label 1', 'value2': 'label2'}" data-ng-model="options.<%= fieldName %>" data-ng-options="key as value for (key, value) in <%= fieldName %>Options" title="<%= fieldLabel %>"></select>
            </div>
